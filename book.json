{
  "gitbook": "3.x.x",
  "title": "Palisade Doc",
  "plugins": [
    "expandable-chapters-interactive",
    "codetabs",
    "prism",
    "-highlight",
    "github",
    "anchorjs",
    "otherlink"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/gchq/palisade"
    },
    "theme-default": {
      "styles": {
        "website": "build/gitbook.css"
      }
    },
    "expandable-chapters": {},
    "lunr": {
      "maxIndexSize": 5000000
    },
    "search": {
      "maxIndexSize": 5000000
    }
  },
  "root": "./",
  "structure": {
    "summary": "contents.md"
  }
}
